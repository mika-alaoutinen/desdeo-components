{
  "extends": [ ":preserveSemverRanges", "config:base" ],
  "automerge": false,
  "groupName": "all dependencies",
  "groupSlug": "all",
  "lockFileMaintenance": {
    "enabled": false
  },
  "packageRules": [
    {
      "groupName": "all dependencies",
      "groupSlug": "all",
      "matchDepTypes": [ "dependencies" ],
      "matchPackagePatterns": [ "*" ],
      "rangeStrategy": "replace"
    }
  ],
  "prCreation": "status-success",
  "separateMajorMinor": true
}